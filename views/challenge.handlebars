      <h4>{{dataValues.Template.dataValues.name}}</h4>
      <h4>Challenge Status: {{dataValues.state}}</h4>
      <div class="collection">
         <h4>Rules:</h4>
         <div class="collection">
            <p>{{dataValues.Template.dataValues.rule}}</p>
         </div>
         {{#if dataValues.state_code}}
